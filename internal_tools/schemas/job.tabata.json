{
  "$schema": "http://json-schema.org/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "NAME": {
        "type": "string"
      },
      "MODE": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "rounds": {
        "type": "integer",
        "default": 8
      },
      "work_time_in_seconds": {
        "type": "integer",
        "default": 20
      },
      "rest_time_in_seconds": {
        "type": "integer",
        "default": 10
      },
      "EXERCISES": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "NAME": {
              "type": "string"
            },
            "reps": {
              "type": "integer"
            },
            "weight": {
              "type": "integer"
            },
            "distance": {
              "type": "integer"
            }
          },
          "required": [
            "NAME"
          ]
        }
      }
    },
    "required": [
      "EXERCISES",
      "NAME",
      "MODE"
    ]
  }
}