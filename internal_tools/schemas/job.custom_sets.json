{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "NAME": {
            "type": "string"
        },
        "MODE": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "Rounds": {
            "type": "integer"
        },
        "Rest_between_exercises_in_seconds": {
            "type": "integer"
        },
        "Rest_between_rounds_in_seconds": {
            "type": "integer"
        },
        "work_time_in_seconds": {
            "type": "integer"
        },
        "work_time_in_minutes": {
            "type": "integer"
        },
        "cadence": {
            "type": "string"
        },
        "Eccentric (NEG)": {
            "type": "boolean"
        },
        "isometric (HOLD)": {
            "type": "boolean"
        },
        "EXERCISES": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "NAME": {
                        "type": "string"
                    },
                    "reps": {
                        "type": "integer"
                    },
                    "work_time_in_seconds": {
                        "type": "integer"
                    },
                    "weight": {
                        "type": "integer"
                    }
                },
                "required": [
                    "NAME"
                ],
                "anyOf": [
          { "required": [ "reps" ] },
          { "required": [ "work_time_in_seconds" ] }
        ]
            }
        }
    },
    "required": [
        "EXERCISES",
        "MODE",
        "NAME",
        "Rounds"
    ]
}