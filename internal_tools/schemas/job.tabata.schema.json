{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "MODE": {
      "type": "string",
      "enum": ["TABATA"]
    },
    "NAME": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "rounds": {
      "type": "integer",
      "default": 8
    },
    "rest_time_in_seconds": {
      "type": "integer",
      "default": 10
    },
    "work_time_in_seconds": {
      "type": "integer",
      "default": 20
    },
    "EXERCISES": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "NAME": {
            "type": "string"
          },
          "reps": {
            "type": "integer"
          },
          "weight": {
            "type": "integer"
          }
        },
        "required": ["NAME", "reps"]
      }
    }
  },
  "required": ["EXERCISES", "NAME", "MODE"]
}